local user_num = "â£ Ø¹Ø¯Ø¯ Ø§Ù„Ø§Ø¹Ø¶Ø§Ø¡: "..result.participants_count.."\n"
local kicked_num = "â£ Ø§Ù„Ø§Ø¹Ø¶Ø§Ø¡ Ø§Ù„Ø§ÙƒØ«Ø± ØªÙØ§Ø¹Ù„: "..result.kicked_count.."\n"
local channel_id = "â£ Ø§ÙŠØ¯ÙŠ Ø§Ù„Ù…Ø¬Ù…ÙˆØ¹Ù‡: "..result.peer_id.."\n"
